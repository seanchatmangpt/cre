{
  "name": "admin-console",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:integration": "vitest --run tests/integration --reporter=verbose",
    "test:integration:watch": "vitest tests/integration",
    "test:deployment": "vitest --run tests/integration/deployment-validation.test.ts",
    "test:upgrade": "vitest --run tests/integration/upgrade-testing.test.ts",
    "test:recovery": "vitest --run tests/integration/failure-recovery.test.ts",
    "test:performance": "vitest --run tests/integration/performance-benchmarks.test.ts",
    "test:load": "vitest --run tests/integration/load-testing.test.ts",
    "test:e2e": "vitest --run tests/integration/e2e-integration.test.ts",
    "test:coverage": "vitest --coverage --run",
    "test:coverage:view": "vitest --coverage --run && open coverage/index.html",
    "test:all": "vitest --run --reporter=verbose --coverage",
    "test:critical": "vitest --run tests/integration --grep='(Deployment|Upgrade|Failure Recovery)'",
    "test:performance-suite": "vitest --run tests/integration/performance-benchmarks.test.ts tests/integration/load-testing.test.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "@tanstack/react-query": "^5.20.0",
    "axios": "^1.6.7",
    "recharts": "^2.12.0",
    "date-fns": "^3.3.0",
    "lucide-react": "^0.323.0",
    "clsx": "^2.1.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.3.3",
    "vite": "^5.1.0",
    "vitest": "^1.2.0",
    "@testing-library/react": "^14.2.0",
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/user-event": "^14.5.2",
    "happy-dom": "^13.3.0"
  }
}
